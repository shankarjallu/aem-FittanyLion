<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Container"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <design
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Layout design"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <backgroundcolor
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Laout background color"
                                        fieldLabel="Layout Background Theme"
                                        name="./layoutbackgroundtheme">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <black
                                                jcr:primaryType="nt:unstructured"
                                                text="black"
                                                value="global-black"/>
                                            <white
                                                jcr:primaryType="nt:unstructured"
                                                text="white"
                                                value="global-white"/>
                                            <christaiangrey
                                                jcr:primaryType="nt:unstructured"
                                                text="Christian Grey"
                                                value="global-christian_grey"/>
                                            <greyskull
                                                jcr:primaryType="nt:unstructured"
                                                text="Grey Skull"
                                                value="global-greyskull"/>
                                            <jeangrey
                                                jcr:primaryType="nt:unstructured"
                                                text="Jean Grey"
                                                value="global-jean_gray"/>
                                            <gardengrey
                                                jcr:primaryType="nt:unstructured"
                                                text="Garden Grey"
                                                value="global-grey_garden"/>
                                            <inthenavy
                                                jcr:primaryType="nt:unstructured"
                                                text="In The Navy"
                                                value="global-in_the_navy"/>
                                            <peacock
                                                jcr:primaryType="nt:unstructured"
                                                text="Peacock"
                                                value="global-mrs_peacock"/>
                                            <highmarkblue
                                                jcr:primaryType="nt:unstructured"
                                                text="Highmark Blue"
                                                value="global-highmark_blue"/>
                                            <ivyblue
                                                jcr:primaryType="nt:unstructured"
                                                text="Ivy Blue"
                                                value="global-blue_ivy"/>
                                            <littleboyblue
                                                jcr:primaryType="nt:unstructured"
                                                text="Littleboy Blue"
                                                value="global-littleboy_blue"/>
                                            <olive
                                                jcr:primaryType="nt:unstructured"
                                                text="Olive"
                                                value="global-martini_olive"/>
                                            <kermit
                                                jcr:primaryType="nt:unstructured"
                                                text="Kermit"
                                                value="global-kermit"/>
                                            <mustard
                                                jcr:primaryType="nt:unstructured"
                                                text="Mustard"
                                                value="global-mustard"/>
                                            <orange
                                                jcr:primaryType="nt:unstructured"
                                                text="Orange"
                                                value="global-orange"/>
                                            <peach
                                                jcr:primaryType="nt:unstructured"
                                                text="Peach"
                                                value="global-peach"/>
                                            <plum
                                                jcr:primaryType="nt:unstructured"
                                                text="Plum"
                                                value="global-plum"/>
                                            <purple
                                                jcr:primaryType="nt:unstructured"
                                                text="Purple"
                                                value="global-purple"/>
                                            <blood
                                                jcr:primaryType="nt:unstructured"
                                                text="Blood"
                                                value="global-blood"/>
                                            <scarlet
                                                jcr:primaryType="nt:unstructured"
                                                text="scarlet"
                                                value="global-scarlet"/>
                                            <pennstateblue
                                                jcr:primaryType="nt:unstructured"
                                                text="penn state blue"
                                                value="global-pennstate-blue"/>
                                        </items>
                                    </backgroundcolor>
                                    <isfullwidth
                                        granite:class="core-image-decorative"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="By checking this, you are making the layout container go full-width"
                                        name="./isFullWidth"
                                        text="Full width?"
                                        value="{Boolean}true"/>
                                    <fixedcontent
                                        granite:class="core-image-decorative"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check this box to restrict everything inside the layout container to 1360px"
                                        name="./isContent"
                                        text="Restrict content inside layout container to 1360px?"
                                        value="{Boolean}true"/>
                                    <hideinmobile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./hideinmobile"
                                        text="Hide in mobile (&lt;768 px)"
                                        value="{Boolean}true"/>
                                    <hideindesk
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./shadowborder"
                                        text="ShadowBorder"
                                        value="{Boolean}true"/>
                                    <shadowborder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./shadowborder"
                                        text="Hide in mobile (&lt;768 px)"
                                        value="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </design>
                    <margin
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Margin"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <isherocomponent
                                        granite:class="core-image-decorative"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="By checking this, you are adding margin of 84px on top of component"
                                        name="./isHeroComponent"
                                        text="Hero Component?"
                                        value="{Boolean}true"/>
                                    <desktop-margin
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Desktop Top and Bottom margin"
                                        name="./desktopmargin">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px margin"
                                                value="desk-ten-margin"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px margin"
                                                value="desk-twenty-margin"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px margin"
                                                value="desk-thirty-margin"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px margin"
                                                value="desk-fourty-margin"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px margin"
                                                value="desk-fifty-margin"/>
                                            <sixty
                                                jcr:primaryType="nt:unstructured"
                                                text="60px margin"
                                                value="desk-sixty-margin"/>
                                            <seventy
                                                jcr:primaryType="nt:unstructured"
                                                text="70px margin"
                                                value="desk-seventy-margin"/>
                                            <eighty
                                                jcr:primaryType="nt:unstructured"
                                                text="80px margin"
                                                value="desk-eighty-margin"/>
                                        </items>
                                    </desktop-margin>
                                    <Desktop-left-right
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Desktop Left and Right Margin"
                                        name="./deskltrtmargin">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px margin"
                                                value="horizontal-ten-margin"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px margin"
                                                value="horizontal-twenty-margin"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px margin"
                                                value="horizontal-thirty-margin"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px margin"
                                                value="horizontal-forty-margin"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px margin"
                                                value="horizontal-fifty-margin"/>
                                            <sixty
                                                jcr:primaryType="nt:unstructured"
                                                text="60px margin"
                                                value="horizontal-sixty-margin"/>
                                        </items>
                                    </Desktop-left-right>
                                    <tablet-top-bottom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Tablet Top and Bottom margin"
                                        name="./tabletmargin">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px margin"
                                                value="tab-ten-margin"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px margin"
                                                value="tab-twenty-margin"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px margin"
                                                value="tab-thirty-margin"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px margin"
                                                value="tab-forty-margin"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px margin"
                                                value="tab-fifty-margin"/>
                                            <sixty
                                                jcr:primaryType="nt:unstructured"
                                                text="60px margin"
                                                value="tab-sixty-margin"/>
                                        </items>
                                    </tablet-top-bottom>
                                    <tablet-left-right
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Tablet Left and Right margin"
                                        name="./tabletltrtmargin">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px margin"
                                                value="tabhorz-ten-margin"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px margin"
                                                value="tabhorz-twenty-margin"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px margin"
                                                value="tabhorz-thirty-margin"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px margin"
                                                value="tabhorz-forty-margin"/>
                                        </items>
                                    </tablet-left-right>
                                    <Mobile-top-bottom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Mobile Top and Bottom margin"
                                        name="./mobilemargin">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px margin"
                                                value="mob-ten-margin"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px margin"
                                                value="mob-twenty-margin"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px margin"
                                                value="mob-thirty-margin"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px margin"
                                                value="mob-forty-margin"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px margin"
                                                value="mob-fifty-margin"/>
                                        </items>
                                    </Mobile-top-bottom>
                                    <Mobile-left-right
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Mobile Left and Right margin"
                                        name="./mobileltrtmargin">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <ten
                                                jcr:primaryType="nt:unstructured"
                                                text="10px margin"
                                                value="mobhorz-ten-margin"/>
                                            <twenty
                                                jcr:primaryType="nt:unstructured"
                                                text="20px margin"
                                                value="mobhorz-twenty-margin"/>
                                            <thirty
                                                jcr:primaryType="nt:unstructured"
                                                text="30px margin"
                                                value="mobhorz-thirty-margin"/>
                                            <forty
                                                jcr:primaryType="nt:unstructured"
                                                text="40px margin"
                                                value="mobhorz-forty-margin"/>
                                            <fifty
                                                jcr:primaryType="nt:unstructured"
                                                text="50px margin"
                                                value="mobhorz-fifty-margin"/>
                                        </items>
                                    </Mobile-left-right>
                                </items>
                            </column>
                        </items>
                    </margin>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
